{
    "name": "vodafone/conf_saturn",
    "description": "Configurador de Ofertas/Descuentos",
    "keywords": ["vodafone", "mars", "api", "microframework", "rest", "router", "psr7"],
    "license": "MIT",
    "authors": [{
        "name": "Diego FeliÃº",
        "email": "diego.feliud@gmail.com"
    }],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "slim/http": "^0.5",
        "illuminate/database": "~5.1",
        "tuupola/cors-middleware": "^0.9.1",
        "vlucas/phpdotenv": "^3.0",
        "seld/jsonlint": "^1.7",
        "firebase/php-jwt": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0",
        "davedevelopment/phpmig": "~1",
        "mazecode/dumpper": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "provide": {
        "psr/log-implementation": "1.0.0"
    },
    "scripts": {
        "start": "php -S 0.0.0.0:8888 -t public public/index.php",
        "post-install-cmd": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "test": "phpunit"
    }
}